const query = require('queryPermission');
const iframe = require('injectHiddenIframe');
const encodeUriComponent = require('encodeUriComponent');

if (data.trackingID == '')
  data.gtmOnFailure();

const url = 'https://ad.propellerads.com/conversion.php?aid=' + encodeUriComponent(data.accountID) + '&pid=&tid=' + encodeUriComponent(data.trackingID) + '&visitor_id=' + encodeUriComponent(data.clickID);
if (query('inject_hidden_iframe', url)) {
  iframe(url, data.gtmOnSuccess, data.gtmOnFailure);
} else {
  data.gtmOnFailure();
}


